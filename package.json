{
  "name": "eternal-deck-generator",
  "version": "0.1.0",
  "description": "Generate decks based on draft tier list",
  "main": "src",
  "scripts": {
    "clean": "shx rm -rf dist",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress",
    "start": "webpack-dev-server --content-base src",
    "deploy": "npm run build && gh-pages-deploy"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:noahsug/eternal-deck-generator.git"
  },
  "keywords": [
    "eternal",
    "card",
    "game",
    "deck",
    "generator",
    "sealed",
    "draft",
    "limited"
  ],
  "author": "Noah Sugarman <noahsug@gmail.com.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noahsug/eternal-deck-generator/issues"
  },
  "homepage": "https://github.com/noahsug/eternal-deck-generator#readme",
  "gh-pages-deploy": {
    "staticpath": "dist"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-preset-airbnb": "^2.4.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.10",
    "csv-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "raw-loader": "^0.5.1",
    "shx": "^0.2.2",
    "style-loader": "^0.20.2",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.1.0"
  }
}
